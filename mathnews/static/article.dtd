<!ENTITY chi      "&#x3C7;">
<!ENTITY Chi      "&#x3A7;">
<!ENTITY psi      "&#x3C8;">
<!ENTITY Psi      "&#x3A8;">
<!ENTITY omega    "&#x3C9;">
<!ENTITY Omega    "&#x3A9;">

<!-- Alternate Names -->
<!ENTITY ldots    "&hellip;">
<!ENTITY ellipsis "&hellip;">

<!-- Structure Classes -->
<!ENTITY % font		"b|i|sans|tt|sup|sub|big|small|mn1|mn2">
<!ENTITY % special	"email|url|editor|special">
<!ENTITY % text		"(#PCDATA|%font;|%special;)*">

<!-- The Structure -->
<!ELEMENT article 	(hd|sh|p|sg|ol|ul|table|pre)*>
<!ELEMENT hd		%text;>
<!ELEMENT sh		%text;>
<!ELEMENT sg		%text;>
<!ELEMENT p 		(#PCDATA|%font;|%special;|sg)*>
<!ELEMENT br 		EMPTY>
<!ELEMENT ol		(li)+>
<!ELEMENT ul		(li)+>
<!ELEMENT li		%text;>
<!ELEMENT pre		%text;>
<!ELEMENT latex		%text;>

<!-- Font Control -->
<!ELEMENT b 		%text;>
<!ELEMENT i 		%text;>
<!ELEMENT sans		%text;>
<!ELEMENT tt		%text;>
<!ELEMENT sup 		%text;>
<!ELEMENT sub 		%text;>
<!ELEMENT big		%text;>
<!ELEMENT small		%text;>
<!ELEMENT special	%text;>
<!ELEMENT u		%text;>

<!-- Specials -->
<!ELEMENT email		(#PCDATA)>
<!ELEMENT url  		(#PCDATA)>
<!ELEMENT editor	%text;>

<!-- Tables -->
<!ELEMENT table		(row)+>
<!ELEMENT row		(cell)+>
<!ELEMENT cell		%text;>

<!-- Attributes -->
<!ATTLIST cell
	colspan		NMTOKEN				"1"
	align		(left|right|centre|center)	#IMPLIED>
<!ATTLIST row
	lineabove	(none|thin|thick)		"none"
	linebelow	(none|thin|thick)		"none">
<!ATTLIST editor
	name		CDATA				"Ed.">
<!ATTLIST li
	value		CDATA				#IMPLIED>
<!ATTLIST special
    class       (mN1|mN2|BB)        "mN1"
	value		CDATA				#IMPLIED>
<!ATTLIST p
	align		(left|right|centre|center)	"left"
	indent		(yes|no)			"yes">
<!ATTLIST pre
	xml:space	(default|preserve)		'preserve'>
<!ATTLIST table
	columns		NMTOKEN				#REQUIRED>
<!ATTLIST ul
	spacing		(single|double)			"double">
<!ATTLIST ol
	spacing		(single|double)			"double">
